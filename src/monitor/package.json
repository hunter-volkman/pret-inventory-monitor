{
  "name": "pret-inventory-monitor",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Real-time inventory monitoring PWA for Pret A Manger",
  "keywords": ["pret", "inventory", "monitoring", "pwa", "viam", "iot"],
  "author": "Viam Solutions Engineering",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host --port 5173",
    "build": "tsc && vite build",
    "preview": "vite preview --host --port 4173",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "test": "echo 'No tests yet' && exit 0"
  },
  "dependencies": {
    "@viamrobotics/sdk": "^0.40.2",
    "js-cookie": "^3.0.5",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/leaflet": "^1.9.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "prettier": "^3.0.0",
    "typescript": "^5.7.2",
    "vite": "^6.3.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}